    "def pairs(list):\n",
    "    newlist={}\n",
    "    for i in range(len(list)):\n",
    "        for j in range(i+1, len(list)):\n",
    "        newlist=newlist+[(list[i],list[j])]\n",
    "    return newlist\n",
    "        \n",
    "pairs([1,2,3,4])"
   ]
  }
